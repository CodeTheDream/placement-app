<h3>Licensee Details</h3>

<div class='card bg-light col-6 rounded mb-0'>
    <div class='card-body'>
        <h4><%= @licensee.facility_name %></h2>
        <p>Contact: <%= @licensee.contact_name %></p>
        <p>Phone: <%= @licensee.phone %></p>
        <p>Fax: <%= @licensee.fax %></p>
        <p><%= @licensee.address %></p>
        <p><%= @licensee.city %>, <%= @licensee.state %> <%= @licensee.zip %></p>
  

        <br>
        <div class='btn btn-secondary'>
            <%= link_to 'Edit', edit_licensee_path(params[:id]) %>
        </div>
        <div class='btn btn-danger'>
            <%= link_to 'Delete', licensee_path(params[:id]), method: :delete %>
        </div>
    </div>
</div>

